# Copy to .env and fill in your credentials
# Never commit this file with real values!

# ============================================================
# OpenClaw Gateway
# ============================================================
OPENCLAW_GATEWAY_TOKEN=your_gateway_token_here
OPENCLAW_CONFIG_DIR=~/.openclaw
OPENCLAW_WORKSPACE_DIR=~/.openclaw/workspace

# ============================================================
# API Proxy (docker-compose.proxy.yml)
# These keys are used by the proxy - agents don't see them
# ============================================================

# Anthropic API (for Claude models)
ANTHROPIC_API_KEY=sk-ant-xxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Telegram Bot API
TELEGRAM_BOT_TOKEN=123456789:ABCdefGHIjklMNOpqrsTUVwxyz

# Brave Search API (optional, for web_search tool)
BRAVE_API_KEY=BSAxxxxxxxxxxxxxxxxxxxxxxxxxx

# GitHub (for git operations and API access)
GITHUB_TOKEN=ghp_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# ============================================================
# Twilio (for WhatsApp)
# ============================================================
TWILIO_ACCOUNT_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_AUTH_TOKEN=your_auth_token_here
# Must be a WhatsApp-enabled Twilio number, prefixed with whatsapp:
TWILIO_WHATSAPP_FROM=whatsapp:+17343367101
